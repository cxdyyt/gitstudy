public interface BankAccount extends Serializable{
    long getAccountNumber();

    char getAccountType();

    BigDecimal getBalance();

    Date getCreatedDate();

    Date getExpiredDate();

    long getOriginalBranchId();

    long getOwnerIdCard();

    void setAccountNumber(long accountNumber);

    void setAccountType(char accountType);

    void setBalance(BigDecimal balance);

    void setCreatedDate(Date createdDate);

    void setExpiredDate(Date expiredDate);

    void setOriginalBranchId(long originalBranchId);

    void setOwnerIdCard(long ownerIdCard);
}
