public interface UserAccount {
    public long getAccountNumber();

    public char getAccountType();

    public BigDecimal getBalance();

    public Date getCreatedDate();

    public Date getExpiredDate();

    public long getOriginalBranchId();

    public long getOwnerIdCard();

    public void setAccountNumber(long accountNumber);

    public void setAccountType(char accountType);

    public void setBalance(BigDecimal balance);

    public void setCreatedDate(Date createdDate);

    public void setExpiredDate(Date expiredDate);

    public void setOriginalBranchId(long originalBranchId);

    public void setOwnerIdCard(long ownerIdCard);
}

public class CreditAccount implements UserAccount {
    /**
     * Unique Card number.
     */
    long accountNumber;

    /**
     * The branch that create the card.
     */
    long originalBranchId;

    /**
     * The id card of the owner
     */
    long ownerIdCard;

    /**
     * Indicate the account type. For example, primary account, secondary account.
     */
    char accountType;

    /**
     * Balance.
     */
    BigDecimal balance;

    /**
     * Card created date
     */
    Date createdDate;

    /**
     * Card expired date
     */
    Date expiredDate;

    public long getAccountNumber() {
        return accountNumber;
    }

    public char getAccountType() {
        return accountType;
    }

    public BigDecimal getBalance() {
        return balance;
    }

    public Date getCreatedDate() {
        return createdDate;
    }

    public Date getExpiredDate() {
        return expiredDate;
    }

    public long getOriginalBranchId() {
        return originalBranchId;
    }

    public long getOwnerIdCard() {
        return ownerIdCard;
    }

    public void setAccountNumber(long accountNumber) {
        this.accountNumber = accountNumber;
    }

    public void setAccountType(char accountType) {
        this.accountType = accountType;
    }

    public void setBalance(BigDecimal balance) {
        this.balance = balance;
    }

    public void setCreatedDate(Date createdDate) {
        this.createdDate = createdDate;
    }

    public void setExpiredDate(Date expiredDate) {
        this.expiredDate = expiredDate;
    }

    public void setOriginalBranchId(long originalBranchId) {
        this.originalBranchId = originalBranchId;
    }

    public void setOwnerIdCard(long ownerIdCard) {
        this.ownerIdCard = ownerIdCard;
    }

}
